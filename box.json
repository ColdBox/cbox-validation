{
    "name":"ColdBox Validation",
    "author":"Ortus Solutions <info@ortussolutions.com>",
    "version":"2.2.0",
    "location":"https://downloads.ortussolutions.com/ortussolutions/coldbox-modules/cbvalidation/@build.version@/cbvalidation-@build.version@.zip",
    "slug":"cbvalidation",
    "type":"modules",
    "homepage":"https://github.com/coldbox-modules/cbvalidation",
    "documentation":"https://coldbox-validation.ortusbooks.com/",
    "repository":{
        "type":"git",
        "url":"https://github.com/coldbox-modules/cbvalidation"
    },
    "shortDescription":"This module provides server side validation to ColdBox applications",
    "license":[
        {
            "type":"Apache2",
            "url":"http://www.apache.org/licenses/LICENSE-2.0.html"
        }
    ],
    "contributors":[
        "Brad Wood <bdw429s@gmail.com>",
        "Curt Gratz <gratz@computerknowhow.com>"
    ],
    "dependencies":{
        "cbi18n":"^1.4.0"
    },
    "ignore":[
        "**/.*",
        "tests/*",
        ".git*"
    ],
    "scripts":{
        "toMaster":"recipe build/toMaster.boxr",
        "format":"cfformat run models/**/*.cfc,ModuleConfig.cfc,tests/specs/**/*.cfc,*.cfc --overwrite",
        "format:check":"cfformat run models/**/*.cfc,ModuleConfig.cfc,tests/specs/**/*.cfc,*.cfc --check",
        "lint":"cflint models/**.cfc --text --html --json --!exitOnError --suppress"
    }
}
